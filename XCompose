include "%L"

# Compose Key
<Multi_key> <minus> <minus> <underscore> : "−" U2212  # MINUS SIGN
<Multi_key> <p> <i> : "π"  # PI
<Multi_key> <exclam> <equal> : "≠"  # NOT EQUAL TO

<Multi_key> <h> <m> <m> : "🤔" U1F914 # THINKING FACE
<Multi_key> <r> <d> <d> <e> : "🙃" U1F643 # Upside-Down Face Emoji
<Multi_key> <s> <m> <i> <r> <k> : "😏"  # Smirking Face 
<Multi_key> <colon> <o> : "😲"  # Astonished Face 
<Multi_key> <s> <h> <r> <u> <g> : "🤷"  # Shrug
<Multi_key> <m> <u> <s> <i> <c> : "🎵"  # Music
<Multi_key> <z> <i> <k> : "🎵"  # Music
<Multi_key> <(> <8> <)> : "🎵"  # Music
<Multi_key> <o> <k> : "👌"
<Multi_key> <o> <k> <s> <i> <g> <n> : "👌"
<Multi_key> <d> <d> <d> : "😀"
<Multi_key> <d> <d> <e> : "🙂"
<Multi_key> <x> <q> <e> : "😅"
<MM> {(y)} : "👍"
<MM> {(nosee)} : "🙈"
<Multi_key> <f> <i> <s> <h> : "🐟"
<Multi_key> <z> <i> <p> : "🤐"
<Multi_key> <o> <h> <e> <a> <r> <t> : "🧡"  # U+1F9E1 ORANGE HEART
<Multi_key> <b> <i> <r> <d> : "🐦"
<Multi_key> <n> <o> <t> : "¬"


<Multi_key> <colon> <three> : "🤭"  # U+1F92D
